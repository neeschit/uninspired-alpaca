{
  "name": "alpaca-play",
  "version": "1.0.0",
  "description": "",
  "dependencies": {
    "@neeschit/alpaca-trade-api": "1.5.0",
    "date-fns": "^2.9.0",
    "date-fns-tz": "^1.0.10",
    "dotenv": "^8.2.0",
    "fastify": "^3.9.2",
    "fastify-cors": "^5.1.0",
    "fs-extra": "^9.0.0",
    "pg": "^8.0.0",
    "pino": "^6.9.0",
    "uuid": "^8.3.2"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^13.13.12",
    "@types/pg": "^7.14.3",
    "@types/uuid": "^7.0.4",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "test:ci": "NODE_ENV=test LOGLEVEL=4 yarn test:jest:ci",
    "test:jest:ci": "yarn test --ci --debug --config jest.config.ci.js",
    "build": "tsc",
    "build:w": "tsc -w",
    "fixDataForToday": "NODE_ENV=backtest node --expose-gc --enable-source-maps build/scripts/symbolDataFixer.js",
    "createSchemas": "node --enable-source-maps build/scripts/createSchemas.js",
    "cacheData": "node --enable-source-maps build/scripts/cacheData.js",
    "cacheFirstBar": "node --enable-source-maps build/scripts/cacheFirstBar.js",
    "getSymbols": "node --enable-source-maps build/scripts/getListOfLargeCaps.js",
    "test": "NODE_ENV=test jest",
    "test:debug": "NODE_ENV=test node --inspect-brk --inspect=9339 node_modules/.bin/jest --runInBand --no-cache",
    "b2": "NODE_ENV=backtest node --inspect=9695 --enable-source-maps build/services/backtest/backtest.js",
    "o2": "SERVICE_NAME=streamer node --inspect=9696 --enable-source-maps build/services/orchestrator-service/index.js",
    "s2": "SERVICE_NAME=screener node --inspect=9698 --enable-source-maps build/services/screener-api/screener.js",
    "t2": "SERVICE_NAME=screener node --inspect=9697 --enable-source-maps build/services/trade-management-api/trade-manager.js"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "build/src/"
      }
    }
  }
}